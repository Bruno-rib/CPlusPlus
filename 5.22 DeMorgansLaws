// pearson c++ how to program 9th edition -- homework 5.22 (De Morgan's Laws) In this chapter
#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
    int x = 9;
    int y = 6;
    
    int a = 1;
    int g = 5;
    int b = 2;
    
    int i = 3;
    int j = 7;
    
    
    cout << !(x < 5) &&  !(y >= 7);
    cout << !((x < 5) || (y >= 7));
    cout << endl;
    
    cout << !(a == b) || !(g != 5);
    cout << !((a == b) && (g != 5));
    cout << endl;
    
    cout << !((x <= 8) && (y > 4));
    cout << !(x <= 8) || !(y > 4);
    cout << endl;
    
    cout << !((i > 4) || (y >= 7));
    cout << !(i > 4) && !(y >= 7);
    cout << endl;
}